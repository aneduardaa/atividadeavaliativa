#include<stdio.h>
  main(){
  int A, B ,B, C;

  printf("digite os respectivos valores");
  scanf("%d %d %d", &A, &B, &C);

  if(A=B > C && A+C > B && B= C> C && B> A){
   printf( " pode ser formado um triangulo");
  }

  float aA, bB, cC;

  if (aA == bB+cC|| bB == aA+cC || cC == aA+bB){
  printf( " pode ser formado um retangulo");
  }else if (aA > bB+cC|| bB > aA+cC || cC > aA+bB){
  printf( " pode ser formado um obtusangulo");
  }else
  printf( " pode ser formado um retangulo");



}
